{
  "manifest_version": 2,
  "name": "FlexPOS",
  "description": "This simple app allows for the browser to be customized in FlexPOS colors.",
  "version": "1.0",
  "author": "AMK.io",
  "chrome_url_overrides": {
    "newtab": "flexpos.html"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://login.flexpos.com/*"
  ]
}